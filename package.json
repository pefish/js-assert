  "scripts": {
    "build": "rm -rf lib && babel src -d lib"
  },
  "main": "lib/assert.js",
  "dependencies": {
    "bignumber.js": "^8.1.1",
    "p-js-error": "^0.0.4"
  },
  "babel": {
