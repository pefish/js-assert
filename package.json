{
  "name": "@pefish/js-helper-assert",
  "version": "0.0.2",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "prepublish": "yarn build"
  },
  "main": "lib/assert.js",
  "dependencies": {
    "bignumber.js": "^8.1.1",
    "p-js-error": "^0.0.4"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "ignore": [
      "node_modules"
    ],
    "env": {
      "build": {
        "optional": [
          "optimisation",
          "minification"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
